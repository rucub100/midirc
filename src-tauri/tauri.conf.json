{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "midirc",
  "version": "0.1.0",
  "identifier": "de.curbanov.midirc.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "midirc",
        "minWidth": 800,
        "width": 1024,
        "minHeight": 600,
        "height": 768,
        "visible": false,
        "windowEffects": {
          "effects": [
            "mica"
          ]
        }
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}